{
    "name": "Copy Tracker",
    "description": "This Extension will keep track of your clipboard",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" },
    "background": {
        "scripts": ["eventPage.js"],
        "persistent": false
      },
      "browser_action": {
        "default_title": "Copy Tracker",
        "default_popup": "popup.html"
      },
      "options_page": "options.html",
    "permissions": [
        "contextMenus",
        "storage"
    ]
  }